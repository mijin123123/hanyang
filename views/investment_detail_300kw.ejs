// ë¡œê·¸ì•„ì›ƒ ì²˜ë¦¬
function handleLogout() {
    if (confirm('ë¡œê·¸ì•„ì›ƒ í•˜ì‹œê² ìŠµë‹ˆê¹Œ?')) {
        logout();
        window.location.href = "/index";
